  "scripts": {
    "build": "npm run clean && gatsby build",
    "clean": "rimraf .cache public",
    "dev": "gatsby develop",
    "proxy": "npx netlify-cms-proxy-server",
    "format": "npm run format:ts && npm run format:scss",
    "format:scss": "stylelint \"src/**/*.scss\" --fix",
    "format:ts": "prettier --write .",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:commit": "commitlint --edit",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint:staged": "lint-staged",
    "lint:ts": "eslint \"src\" --ext .tsx,.ts && prettier --check .",
    "proxy-netlify": "npx netlify-cms-proxy-server",
    "semantic-release": "semantic-release",
    "serve": "gatsby serve",
    "start": "concurrently --kill-others \"npm run clean && gatsby develop\" \"npm run proxy-netlify\"",
    "test": "jest --config ./internal/testing/jest-config.ts",
    "test:coverage": "jest --coverage --config ./internal/testing/jest-config.ts"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{tsx?,ts?}": [
      "npm run format:ts"
    ],
    "*.{css?,scss?}": [
      "npm run format:scss"
    ]
  },
